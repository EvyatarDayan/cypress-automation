{
  "extends": ["airbnb-base", "plugin:chai-friendly/recommended"],
  "plugins": ["import", "cypress", "chai-friendly"],
  "env": {
    "jest": true,
    "es6": true,
    "node": true,
    "cypress/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "comma-dangle": 0,
    "object-curly-newline": 0,
    "consistent-return": 0
  }
}
